
let fakeNew = document.querySelectorAll('.noticiaFalsa');
let realNew = document.querySelectorAll('.noticiaReal');
console.log(realNew);

Array.from(realNew).forEach((mouseOverReal)=>{
 document.addEventListener("mouseover",mouseOverReal);
 });

Array.from(realNew).forEach((mouseOutReal)=>{
    document.addEventListener("mouseout",mouseOutReal);
    });


function mouseOverReal(){
    realNew.style.transform= "scale(1.1)";
}
function mouseOutReal(){
    realNew.style.transform= "scale(1)";
}




//////////////////////////////////////////////////////////

let fakeNew = document.getElementById('noticiaFalsa');
    let realNew = document.getElementsByClassName('noticiaReal');
    console.log(realNew);


    document.addEventListener("mouseover",mouseOverReal);
    document.addEventListener("mouseover",mouseOutReal);

    function mouseOverReal(){
        document.("noticiaReal").style.transform= "scale(1.1)";
    }
    function mouseOutReal(){
        document.getElementsByName("noticiaReal").style.transform= "scale(1)";
    }

    ///////////////////////////////////////////////


let fakeNew = document.getElementById('noticiaFalsa');
    let realNew = document.querySelectorAll('.noticiaReal');
    console.log(realNew);


    realNew.onmouseover= function(){
        realNew.classList.add('scaleUp');
    }
    realNew.onmouseout= function(){
        realNew.classList.remove('scaleUp');
    }

