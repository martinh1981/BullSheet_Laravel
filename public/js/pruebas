///////////////////////////////////////////
window.onload=function(){
let fakeNew = document.querySelectorAll('.noticiaFalsa');
let realNew = document.querySelectorAll('.noticiaReal');
console.log(realNew);

for (var i = 0; i < realNew.length; i++){
    document.addEventListener("mouseover", scaleUp)
    document.addEventListener("mouseout", scaleDown)
}

function scaleUp(){
    realNew[0].style.transform= "scale('1.1')"
}

function scaleDown(){
    realNew[0].style.transform= "scale('1')"
}
}

//////////////////////////////////////////

let fakeNew = document.querySelectorAll('.noticiaFalsa');
const realNew = document.getElementById('noticiaReal');
console.log(realNew);

for (const realNews of realNew){
    this.addEventListener("mouseover", function(mouseover){
        realNew.style.transform= "scale('1.1')"
    })
    document.addEventListener("mouseout", function(mouseout){
        realNew.style.transform= "scale('1')"
    })
}

////////////////////////////////////

let fakeNew = document.querySelectorAll('.noticiaFalsa');
let realNew = document.getElementById('noticiaReal');
console.log(realNew);

document.addEventListener("mouseover", scaleUp);
document.addEventListener("mouseleave", scaleDown);


function scaleUp(){
    realNew.style.transform = "scale(1.1)";
}

function scaleDown(){
    realNew.style.transform = "scale(1)";
}

////////////////////////////////////////////////

window.onload= function(){



}

let fakeNew = document.querySelectorAll('.noticiaFalsa');
let realNew = document.querySelectorAll('.noticiaReal');
console.log(realNew);

document.addEventListener("mouseover", scaleUp);



function scaleUp(){
  if(realNew.classList.contains('scaleUp')){
      realNew.classList.remove('scaleUp');
  }else{
      realNew.classList.add('scaleUp')
  }
}


