let fakeNew = document.querySelectorAll('.noticiaFalsa');
let realNew = document.querySelectorAll('div #noticiaReal');
let i;
console.log(realNew);

for(i=0; i<realNew.length; i++){
 realNew[i].addEventListener("mouseover",mouseOverReal);
 realNew[i].addEventListener("mouseout",mouseOutReal);
}


function mouseOverReal(){
    realNew.style.transform= "scale(1.1)";
}
function mouseOutReal(){
    realNew.style.transform= "scale(1)";
}

