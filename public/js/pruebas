let fakeNew = document.querySelectorAll('.noticiaFalsa');
let realNew = document.querySelectorAll('.noticiaReal');
let i;
console.log(realNew);

Array.from(realNew).forEach(()=>{
 document.addEventListener("mouseover",mouseOverReal);
 });

Array.from(realNew).forEach(()=>{
    document.addEventListener("mouseout",mouseOutReal);
    });


function mouseOverReal(){
    realNew[1].style.transform= "scale(1.05)";
}
function mouseOutReal(){
    realNew[1].style.transform= "scale(1)";
}

//////////////////////////////////////////////

window.onload= function(){
let fakeNew = document.querySelectorAll('.noticiaFalsa');
let realNew = document.querySelectorAll('.noticiaReal');

console.log(realNew);

for (let i=0;i<realNew.length; i++){
 document.addEventListener("mouseover",mouseOverReal);
 document.addEventListener("mouseout",mouseOutReal);
 };



function mouseOverReal(){
    realNew[0].style.transform= "scale(1.05)";
}
function mouseOutReal(){
    realNew[0].style.transform= "scale(1)";
}
}

//////////////////////////////////////////////////

let fakeNew = document.querySelectorAll('.noticiaFalsa');
let realNews = Array.prototype.slice.apply(document.querySelectorAll('.noticiaReal'));

console.log(realNews);

realNews.forEach((realNew) => {
    document.addEventListener("mouseover",mouseOverReal);
    document.addEventListener("mouseout",mouseOutReal);
  });


function mouseOverReal(){
    realNew[realNew].style.transform= "scale(1.05)";
}
function mouseOutReal(){
    realNews[realNew].style.transform= "scale(1)";
}

